{% extends 'base.html.twig' %}

{% block stylesheets %} <link href="{{ asset('css/etablir_contrats.css') }}" type="text/css" rel="stylesheet"> {% endblock %}

{% block body %}

<h1>Etablir les contrats</h1>

<div id="body">

    <div id="container_button" >
        
        <button>Télécharger le contrat</button> 
        <br/>
        <button>Créer le contrat</button> 
        <br/>
        <button>Modifier le contrat</button> 
        <br/>
        <button>Supprimer le contrat</button>

    </div>


    <p> Liste des étudiants  </p>
    <ul>
        {% for etudiant in etudiants %}
            <li>{{etudiant.nom}} {{etudiant.prenom}}</li>
        {% endfor %}
    </ul>
</div>


{% endblock %}